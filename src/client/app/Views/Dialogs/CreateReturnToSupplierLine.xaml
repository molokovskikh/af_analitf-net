<UserControl x:Class="AnalitF.Net.Client.Views.Dialogs.CreateReturnToSupplierLine"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	MinWidth="200">
	<StackPanel Margin="4">
		<Label>Товарный остаток</Label>
		<ComboBox Name="StockName"
			ItemsSource="{Binding Stocks}"
			SelectedItem="{Binding ReturnToSupplierLine.Stock}" DisplayMemberPath="Product" IsEditable="True" />
		<Label>Количество</Label>
		<TextBox Name="ReturnToSupplierLine_Quantity" />
		<StackPanel Orientation="Horizontal" Margin="0, 10, 0, 10">
			<Button Name="OK" Margin="0, 0, 5, 0">ОК</Button>
			<Button Name="TryClose">Отмена</Button>
		</StackPanel>
	</StackPanel>
</UserControl>
