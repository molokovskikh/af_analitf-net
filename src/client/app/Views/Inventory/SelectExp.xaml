<UserControl x:Class="AnalitF.Net.Client.Views.Inventory.SelectExp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AnalitF.Net.Client.Views.Inventory"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
	<StackPanel Margin="4">
		<Label Name="Name" />
		<ComboBox Style="{StaticResource Select}"
			ItemsSource="{Binding Exps.Value}"
			SelectedItem="{Binding CurrentExp}" />
		<StackPanel Orientation="Horizontal" Margin="0, 10, 0, 10">
			<Button Name="OK" Margin="0, 0, 5, 0">ОК</Button>
			<Button Name="TryClose">Отмена</Button>
		</StackPanel>
	</StackPanel>
</UserControl>
