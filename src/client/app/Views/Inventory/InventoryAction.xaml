<UserControl x:Class="AnalitF.Net.Client.Views.Inventory.InventoryAction"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:AnalitF.Net.Client.Views.Inventory"
	mc:Ignorable="d"
	d:DesignHeight="300" d:DesignWidth="300" Padding="7">
		<StackPanel>
			<Label>Выберите операцию</Label>
			<Button Margin="3" Padding="3" Name="CreateNew">Создать новый документ</Button>
			<Button Margin="3" Padding="3" Name="SelectExisting">Выбрать существующий документ</Button>
		</StackPanel>
</UserControl>
